{
    "version": 2,
    "routes": [
      {
        "src": "/books",
        "methods": ["GET"],
        "dest": "index.js"
      },
      {
        "src": "/books/:bookId/",
        "methods": ["GET"],
        "dest": "index.js"
      },
      {
        "src": "/authors/:authorId/",
        "methods": ["GET"],
        "dest": "index.js"
      },
      {
        "src": "/ratings/:rating/",
        "methods": ["GET"],
        "dest": "index.js"
      }
    ],
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ]
  }
  